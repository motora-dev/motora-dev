{
  "name": "@monorepo/client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "find . -type d \\( -name \".next\" -o -name \".turbo\" -o -name \"coverage\" -o -name \"node_modules\" -o -name \"tsconfig.spec.tsbuildinfo\" -o -name \"tsconfig.tsbuildinfo\" \\) -prune -exec rm -rf '{}' +",
    "build": "next build",
    "start": "next dev -p 3000",
    "start:prd": "next start -p 3000",
    "storybook": "storybook dev -p 6006",
    "check-types": "pnpm check-types:src && pnpm check-types:spec",
    "check-types:src": "tsc --noEmit -p tsconfig.json",
    "check-types:spec": "tsc --noEmit -p tsconfig.spec.json",
    "format": "prettier --check \"src/**/*.tsx\"",
    "format:fix": "prettier --write \"src/**/*.tsx\"",
    "lint": "pnpm lint:eslint && pnpm lint:style",
    "lint:fix": "pnpm lint:eslint:fix && pnpm lint:style:fix",
    "lint:eslint": "eslint src --config eslint.config.mjs --max-warnings 0",
    "lint:eslint:fix": "pnpm lint:eslint --fix",
    "lint:style": "stylelint \"src/**/*.css\"",
    "lint:style:fix": "pnpm lint:style --fix",
    "test": "pnpm test:jest && pnpm test:storybook",
    "test:coverage": "pnpm test:jest:coverage && pnpm test:storybook:coverage",
    "test:jest": "jest",
    "test:jest:coverage": "COLLECT_COVERAGE=true jest",
    "test:storybook": "vitest run --project=storybook",
    "test:storybook:coverage": "pnpm test:storybook --coverage"
  },
  "dependencies": {
    "@monorepo/error-code": "workspace:*",
    "@monorepo/markdown": "workspace:*",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-form": "^1.23.7",
    "@tanstack/react-query": "catalog:",
    "@tiptap/core": "catalog:",
    "@tiptap/extension-code-block-lowlight": "catalog:",
    "@tiptap/extension-image": "catalog:",
    "@tiptap/extension-placeholder": "catalog:",
    "@tiptap/pm": "catalog:",
    "@tiptap/react": "catalog:",
    "@tiptap/starter-kit": "catalog:",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "google-auth-library": "catalog:",
    "hast-util-to-html": "^9.0.5",
    "highlight.js": "^11.11.1",
    "ky": "catalog:",
    "lowlight": "^3.3.0",
    "lucide-react": "^0.552.0",
    "next": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:",
    "tailwind-merge": "^3.3.1",
    "zod": "catalog:",
    "zustand": "catalog:"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/eslintrc": "^3.3.1",
    "@feature-sliced/eslint-config": "^0.1.1",
    "@monorepo/eslint-config": "workspace:*",
    "@monorepo/typescript-config": "workspace:*",
    "@storybook/addon-a11y": "catalog:",
    "@storybook/addon-vitest": "catalog:",
    "@storybook/nextjs-vite": "catalog:",
    "@tailwindcss/postcss": "^4.1.14",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "catalog:",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitest/browser": "catalog:",
    "@vitest/browser-playwright": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "eslint-config-next": "^16.0.1",
    "eslint-plugin-feature-sliced": "^0.0.2",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-storybook": "10.0.2",
    "eslint-plugin-testing-library": "^7.13.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "catalog:",
    "jest-environment-jsdom": "catalog:",
    "playwright": "catalog:",
    "storybook": "catalog:",
    "stylelint": "catalog:",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-tailwindcss": "^1.0.0",
    "tailwindcss": "catalog:",
    "ts-jest": "^29.4.5",
    "tw-animate-css": "^1.4.0",
    "vitest": "catalog:"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix --config eslint.config.mjs"
    ],
    "src/**/*.css": [
      "prettier --write",
      "stylelint --fix"
    ]
  },
  "volta": {
    "extends": "../../package.json"
  }
}
